{
  "name": "toolset/Idcard",
  "description": "根据提供的身份证号码，检测身份证号码是或否正确，以及解读身份证号码中所包含的年龄、地域等信息",
  "type": "library",
  "homepage": "https://github.com/PerterWan/idcard",
  "version": "1.0.3",
  "license": "MIT",
  "authors": [
    {
      "name": "PerterWan",
      "email": "wysszetz@163.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7"
  },
  "autoload-dev": {
    "psr-4": {
      "Idcard\\Tests\\": "tests/"
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    }
  },
  "autoload": {
    "psr-4": {
      "Idcard\\": "src/"
    }
  }
}
